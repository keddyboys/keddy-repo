<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.filmeserialeonlineorg"
       name="Filmeserialeonline.org"
       version="1.0.0"
       provider-name="keddy">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.urlresolver" version="4.0.09"/>
    <import addon="script.mrknow.urlresolver" version="2016.04.06.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Filme si seriale subtitrate de pe Filmeserialeonline.org</summary>
    <description>Vizioneaza filmele si serialele subtitrate disponibile pe Filmeserialeonline.org</description>
    <platform>all</platform>
    <news>First version</news>
  </extension>
</addon>
<addon id="repository.keddy.repo" name="Keddy's Addons" version="1.0.2" provider-name="keddy">
	<extension point="xbmc.addon.repository" name="Keddy's Addons">
		<info compressed="false">https://raw.githubusercontent.com/keddyboys/keddy-repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/keddyboys/keddy-repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/keddyboys/keddy-repo/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Addons from https://github.com/keddyboys/keddy-repo</summary>
		<description>Download and install addons for Kodi.</description>
                <disclaimer>Keddy</disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>